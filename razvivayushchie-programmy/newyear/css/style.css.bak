*
{
    margin: 0;
    padding: 0;
}

body
{
    font-family: CoreRhino25Thin;

    color: #000;
    background: #2f3860;
}

.container-fluid
{
    padding: 0;
}

#nav
{
    position: fixed;
    top: 45%;
    right: 2%;

    color: #000;
}


.next-control
{
    font-size: 2.1em;

    position: fixed;
    bottom: -4px;
    left: 50%;

    width: 26px;

    text-align: center;

    color: #000;;
}
a
{
    text-decoration: none;

    color: #7fb6d8;
}
a:focus,
a:hover
{
    text-decoration: underline;

    color: #a8609e;
}
a:active
{
    color: #a8609e;
}

#fp-nav ul li .fp-tooltip
{
    font-family: CoreRhino25Thin;
    font-size: 14px;

    position: absolute;
    top: -2px;

    display: block;
    overflow: hidden;

    width: 0;
    max-width: 220px;

    cursor: pointer;
    white-space: nowrap;

    opacity: 0;
    color: #64c8a4;
}

#fp-nav ul li a span,
.fp-slidesNav ul li a span
{
    position: absolute;
    z-index: 1;
    top: 50%;
    left: 50%;

    width: 4px;
    height: 4px;
    margin: -2px 0 0 -2px;

    -webkit-transition: all .1s ease-in-out;
            transition: all .1s ease-in-out;

    border: 0;
    border-radius: 50%;
    background: #64c8a4;
}

h1.title
{
    font-family: 'CoreRhino85Black';
    font-size: 40px;
    font-weight: 400;

    color: #ad5a95;
}
h3.subtitle
{
    font-family: 'CoreRhino45Regular';
    font-size: 26px;
    font-weight: 400;

    color: #4c4c4c;
}

.buy-btn
{
    font-family: 'CoreRhino45Regular';
    font-size: 20px;

    position: relative;
    top: 120px;

    display: block;

    width: 200px;
    margin: 0 auto;

    text-align: center;

    color: white;
    border-radius: 6px;
    background-color: #64c8a4;
}


.buy-btn:hover
{
    -webkit-transition: all .6s ease-in-out;
            transition: all .6s ease-in-out;

    color: #fff;
    background-color: #a8609e;
}




