/* figures of prod */
#monsiki-figures
{
    position: absolute;
    z-index: 0;
    left: -80px;

    width: 100%;
    height: 1080px;
    margin: 0;
    padding: 0;

    white-space: nowrap;

    background: #2f3860;
}



#monsiki-figures div.figureleft,
#monsiki-figures div.figureright,
#monsiki-figures div.figurecenter
{
    display: inline-block;

    width: 36%;
    min-height: 1080px;

    -webkit-transition: all .6s ease-in-out;
            transition: all .6s ease-in-out;
    -webkit-transform: skewX(-10deg);
            transform: skewX(-10deg);
}
#monsiki-figures div.figureleft:hover,
#monsiki-figures div.figureright:hover,
#monsiki-figures div.figurecenter:hover
{
    cursor: pointer;

    border-right: 8px solid white;
    border-left: 8px solid white;
    background: #fff;
}
img.monsta-1:hover,
img.monsta-2:hover,
img.monsta-3:hover
{
    cursor: pointer;
}




#monsiki-figures .figureleft
{
    width: 100%;

    background: #febc11;
}
#monsiki-figures .figurecenter
{
    width: 38%;

    background: #7b65ac;
}
#monsiki-figures .figureright
{
    background: #f05381;
}

